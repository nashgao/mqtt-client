  "agent_registry": {
    "problem_investigator": {
      "name": "problem-investigator",
      "description": "Comprehensive problem investigation agent using multi-layered analysis and research coordination",
      "capabilities": [
        "Symptom analysis and classification",
        "Root cause discovery using 5 Whys and Fishbone analysis",
        "Impact assessment and risk evaluation",
        "Evidence collection and validation",
        "Solution synthesis and fix recommendations",
        "Multi-agent research coordination"
      ],
      "trigger_patterns": [
        "Bug investigation",
        "Error diagnosis",
        "System problem analysis",
        "Performance issue investigation",
        "Integration failure debugging"
      ],
      "best_for": "Complex bugs, production issues, systematic problem analysis"
    }
  },
  "command_registry": {
    "investigate": {
      "name": "/investigate",
      "description": "Deploy specialized investigation agents for systematic problem analysis",
      "usage": "/investigate <problem description>",
      "modes": {
        "standard": "Comprehensive 5-phase investigation",
        "critical": "Emergency response mode with immediate stabilization",
        "deep": "Extended analysis with historical patterns"
      },
      "agents_used": [
        "problem-investigator",
        "research-orchestrator",
        "infra-context-discovery",
        "specialized agents based on problem type"
      ],
      "output": "Investigation report with root cause, evidence, and solutions",
      "execution_time": "5-15 minutes standard, 2-5 minutes critical"
    }
  },
  "pattern_replacements": {
    "data_models": {
      "pattern": "class (\\w+) \\{[^}]*\\}",
      "replacement": "class $1 extends Entity { }",
      "description": "Convert plain classes to Entity pattern"
    },
    "array_operations": {
      "pattern": "array_map\\((.+?)\\)",
      "replacement": "Collection::from($1)->map($2)",
